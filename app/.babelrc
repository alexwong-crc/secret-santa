{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "alias": {
          "@/src": "./src",
          "@/components": "./src/components",
          "@/atoms": "./src/components/atoms"
        }
      }
    ]
  ]
}
